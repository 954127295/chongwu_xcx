.map_container{ 
    height: 300px; 
    width: 100%; 
} 

.map { 
    height: 100%; 
    width: 100%; 
}
.gpstext{
	color:#1AAD19;
}
.wx-switch-input{
	width:82rpx !important;
	height:40rpx !important;
}
/*白色样式（false的样式）*/
.wx-switch-input::before{
	width:80rpx !important;
	height:36rpx !important;
}
/*绿色样式（true的样式）*/
.wx-switch-input::after{
	width:38rpx !important;
	height:36rpx !important;
}

.battery_all{
	width:100%;
	height:50rpx;
}
.gps_on_off{
	float:left;
	margin-left:20rpx;
}
.battery{
	width:90rpx;
	height:26rpx;
	z-index:10000;
	display:flex;
	flex-direction:row;
	float:right;
	margin-right:20rpx;
	margin-top:2%;
	margin-top:10rpx;
}

.battery1{
	border:1px solid #1AAD19;
	width:87rpx;
	height:100%;
	border-radius:5rpx;
}
.battery2{
	border:1px solid #1AAD19;
	width:3rpx;
	height:18rpx;
	margin-top:4rpx;
	border-top-right-radius:1em;
	border-bottom-right-radius:1em;
	background-color:#1AAD19;
}